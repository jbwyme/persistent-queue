{
  "name": "persistent-queue",
  "version": "1.0.0",
  "description": "A simple, cross-browser queue that persists between page loads using multiple storage strategies for compatibility",
  "main": "build/index.cjs.js",
  "jsnext:main": "build/index.es6.js",
  "scripts": {
    "build": "./node_modules/rollup/bin/rollup -c rollup.config.cjs.js && ./node_modules/rollup/bin/rollup -c rollup.config.es6.js",
    "lint": "./node_modules/eslint/bin/eslint.js ./src ./tests",
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register tests/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbwyme/persistent-queue.git"
  },
  "keywords": [
    "javascript",
    "queue",
    "persistent",
    "queue",
    "client-side",
    "queue"
  ],
  "author": "Josh Wymer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jbwyme/persistent-queue/issues"
  },
  "homepage": "https://github.com/jbwyme/persistent-queue#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "chai": "^3.5.0",
    "eslint": "^2.10.1",
    "jsdom": "^9.0.0",
    "mocha": "^2.4.5",
    "mocha-jsdom": "^1.1.0",
    "rollup": "^0.26.3",
    "rollup-plugin-babel": "^2.4.0",
    "sinon": "^1.17.4"
  }
}
